<div class="page" id="page-front">

    <header class="entete col18 pagecnx" id="bandeau">

        <!-- Avoidance Links -->
        <div id="evitement" class="invis">
            <ul>
                <li><a href="#menu" tabindex="1" class="evitement">Aller au menu</a></li>
                <li><a href="#content" tabindex="2" class="evitement" accesskey="s">Aller au contenu</a></li>
                <li><a href="<%= link.search %>" tabindex="3" accesskey="4" class="evitement" target="_blank">Aller &agrave; la recherche</a></li>
            </ul>
            <hr>
        </div>

        <!-- Hidden header for "Mes Espaces" -->
        <% include ./partials/header1 %>

        <hr class="ligne-sous-menu" id="line-sous"/>

        <div class="header2" id="id-header2">
            <div class="header-container">

                <!-- Right column with logo-->
                <div class="logo col6">
                    <a id="home-link" href="<%= link.home %>" target="_blank">
                        <img alt="" src="data:image/png;base64,"/>
                    </a>


                    <div class="select" id="select">
                        <h2>
                            <a>
                                <span class="text-uppercase text-white text-italic text-left text-block">Intranet</span>
                                <span class="text-uppercase text-white text-bold-italic text-left text-block">Services-courrier-colis</span>
                            </a>
                        </h2>
                        <div class="select-dropdown">
                            <a href="#">
                                <span class="text-uppercase text-white text-italic text-left text-block">Intranet</span>
                                <span class="text-uppercase text-white text-bold-italic text-left text-block">Services-courrier-colis</span>
                            </a>
                        </div>
                    </div>
                </div>

                <!-- Middle column with "espace" button -->
                <div class="espace col6">
                    <button href="#0" tabindex="4" aria-expanded="false" class="espace-button" id="espace">
                        <span class="text-uppercase text-white text-bold text-center">Les espaces</span>
                    </button>
                </div>

                <!-- Right column with search, authentication and applications -->
                <div class="col6 link-right">
                    <ul class="rightheader" id="header-right">

                        <!-- Login icon with login form and notification block -->
                        <li>
                            <% include ./partials/popover_block %>
                        </li>

                        <!-- Search form -->
                        <li>
                            <a href="<%= link.search %>" class="search" id="recherche" target="_blank">

                                <img alt="Recherche" src="data:image/png;base64,"/>
                            </a>
                            <div role="search" class="search-div profil" id="search">
                                <form>
                                    <label for="recher-input" id="recher-label">Entrez votre texte...</label>
                                    <input type="text" title="Recherche" class="recher-input" id="recher-input" maxlength="100">
                                </form>
                                <button class="trouver">Trouver</button>
                            </div>
                        </li>

                        <!-- List of applications -->
                        <li id="list-of-applications" class="dernier">
                            <a href="#0" id="application-id" class="application">
                                <img alt="Applications" src="data:image/png;base64,"/>
                            </a>
                            <div id="liste-appli" class="prof profil">

                                <div class="entete-appli"></div>
                                <ul id="appl">
                                    <!-- Application list goes here -->
                                </ul>
                                <a href="<%= link.wac %>" id="wac-link" class="ttapplica" target="_blank"><span class="text-uppercase text-bold text-center">Accéder à Toutes les applis</span></a>
                                <div class="dragging-over">
                                    <p>Glisser ici pour <br> Ajouter aux Favoris</p>
                                </div>

                            </div>
                            <div id="app-notification">
                                <span class="add">l’application a bien été ajoutée<br>à vos Favoris.</span>
                                <span class="remove">l’application a bien été retirée<br>de vos Favoris.</span>
                                <span class="toomuch">vous avez trop d'application<br>dans vos Favoris.</span>
                            </div>
                        </li>

                    </ul>
                </div>
            </div>
        </div>
    </header>

    <!-- Main content of the page -->
    <% include ./partials/main %>

    <% include ./backbone/notification_block %>
    <% include ./backbone/login_form %>
    <% include ./backbone/login_icon_status %>

    <% if(auto_connect && is_first_visit && !(is_authenticated)){ %>

    <iframe height="0" width="0" src="<%= prefix + '/sp/login'%>"/>

    <% } %>


</div>

<div class="overlay" id="overlay-body"></div>
